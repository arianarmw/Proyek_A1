<%- include('layout/header') %>

<div class="container">
    <div class="row my-4">
        <div class="col-lg-12">
            <% if (message) { %>
                <div class="alert alert-dismissible fade show alert- <%= message.type %>" role="alert">
                    <button class="btn-close" type="button" data-bs-dismiss="alert" aria-label="close"></button>
                    <strong><%= message.message %></strong>
                </div>
            <% } %>
            <div class="table-responsive">
                <table class="table table-striped text-center">
                    <thead>
                        <tr class="table-dark">
                            <th>Logo</th>
                            <th>Alamat</th>
                            <th>Produk</th>
                            <th>Rating</th>
                            <th>Kategori</th>
                            <th>Berkas</th>
                            <th>Fasilitas</th>
                            <th>Jam Buka</th>
                            <th>Jam Tutup</th>
                            <th>Status</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <trbody>
                        <tr>
                            <td><img src="" alt=""></td>
                            <td>Oxford OX1 2JD, England</td>
                            <td>Emping</td>
                            <td>5/5</td>
                            <td>Indonesian</td>
                            <td></td>
                            <td>Wi-Fi, Toilet</td>
                            <td>9:40 AM</td>
                            <td>10:00 PM</td>
                            <td>Aktif</td>
                            <td>
                                <a href="" class="text-success"><i class="fas fa-edit fa-lg mx-1"></i></a>
                                <a href="" class="text-danger"><i class="fas fa-trash fa-lg mx-1"></i></a>
                            </td>
                        </tr>
                    </trbody>
                </table>
            </div>
        </div>
    </div>

</div>
<%- include('layout/footer') %>